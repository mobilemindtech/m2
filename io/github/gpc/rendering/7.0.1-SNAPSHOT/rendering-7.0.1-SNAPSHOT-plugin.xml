    <resource>grails.plugins.rendering.image.GifRenderingService</resource>
    <resource>grails.plugins.rendering.image.ImageRenderingService</resource>
    <resource>grails.plugins.rendering.image.JpegRenderingService</resource>
    <resource>grails.plugins.rendering.image.PngRenderingService</resource>
