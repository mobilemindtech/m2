<plugin name='rendering' version='7.0.1-SNAPSHOT' grailsVersion='7.0 &gt; *'>
  <type>grails.plugins.rendering.RenderingGrailsPlugin</type>
  <authorEmail>grails.plugin.collective@gmail.com</authorEmail>
  <author>Grails Plugin Collective</author>
  <documentation>https://gpc.github.io/rendering/</documentation>
